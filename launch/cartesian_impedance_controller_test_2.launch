<?xml version="1.0" ?>
<launch>




 <include file="$(find franka_gazebo)/launch/panda.launch" >
    <arg name="rviz" value="true" />
    <arg name="controller" value=" " />
  </include>
  <rosparam command="load" file="$(find cartesian_impedance_controller)/config/franka_impedance_controller.yaml" />


 

 <node pkg="controller_manager"
        type="spawner"
        name="$(arg arm_id)_controller_spawner2"
        respawn="false" output="screen"
        args="CartesianImpedance_trajectory_controller"
  />


  <node name="interactive_marker" pkg="franka_example_controllers" type="interactive_marker.py" required="true" output="screen">
    <param name="link_name" value="panda_link0" />
    <remap from="equilibrium_pose" to="/CartesianImpedance_trajectory_controller/reference_pose" />
  </node>
